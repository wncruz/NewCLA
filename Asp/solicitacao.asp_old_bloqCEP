		'Response.Write "<script language=javascript>alert('" & dblSolId & " - " & dblSolAPGId &  "');</script>"
	End If
Else

	dblSolId = Trim(Request.Form("hdnSolId"))

End If

'Response.Write "<script language=javascript>alert('Origem: " & strOrigem & ", " & dblSolAPGId &  "');</script>"

If Trim(Request.Form("hdnAcao")) = "Alteracao"  Or  strOrigem = "APG" then

	If Trim(Request.Form("hdnAcao")) = "Alteracao"  And (dblSolId = "" or dblSolId = "0") Then

	'if dblSolId = "" then
