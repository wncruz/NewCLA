	'response.write "<h1>"&dblSolAPGId&"</h1>" '@@DEBUG
	'FIM - T3ENA 09/04/2004
	
	If Not objRSSolic.eof or Not objRSSolic.bof Then
		dblSolId = trim(objRSSolic("solicitacao"))
		dblIdLogico = trim(objRSSolic("ID_logico"))
