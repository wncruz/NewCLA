		Response.End

	Else

		If strOrigem = "APG" and dblSolAPGId = "" Then

			Response.Write "<script language=javascript>alert('Solicitação indisponível.');window.location.replace('solicitacao_main.asp');</script>"

		End IF

	End if

	'Adicionado PRSS 29/01/2007:
	strSQL = "select Ctf_ID from cla_solicitacaoctf where sol_id = " & dblSolId
    Set objRSCtfc = db.execute(strSQL)
	dblCtfcId = objRSCtfc("Ctf_ID")

	'@@Davif - Interface APG
	'Alterado T3ENA - 09/04/2007 - INICIO
	If StrOrigem <> "APG" Then

		'Set objRSSolic = db.execute("CLA_sp_sel_tarefas_APG null, null, null, " & dblSolAPGId)

