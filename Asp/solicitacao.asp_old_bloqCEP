      End if
	End If

	if objRSSolic.Eof then
		Response.Write "<script language=javascript>alert('Solicitação indisponível.');window.location.replace('main.asp');</script>"
		Response.End
	End if

	'@@Davif - Interface APG
	If Trim(Request.Form("hdnAcao")) = "Alteracao" Then

